        "lint:fix": "eslint --color --ext .js,.jsx,.ts,.tsx src --quiet --fix",
        "prettier": "prettier --write ."
    },
    "dependencies": {
        "@ant-design/charts": "^1.3.6",
        "@ant-design/icons": "^4.7.0",
        "@ant-design/pro-card": "^1.20.11",
        "@ant-design/pro-descriptions": "^1.11.7",
        "@ant-design/pro-form": "^1.70.0",
        "@ant-design/pro-layout": "^6.38.10",
        "@ant-design/pro-list": "^1.21.70",
        "@ant-design/pro-skeleton": "^1.0.9",
        "@ant-design/pro-table": "^2.77.0",
        "antd": "^4.21.5"
    },
    "devDependencies": {
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "autoprefixer": "^10.4.7",
        "less": "^4.1.3",
        "path": "^0.12.7",
        "postcss": "^8.4.14",
        "sass": "^1.53.0",
        "tailwindcss": "^3.1.4",
        "typescript": "^4.7.4",
        "vite": "^2.9.13",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "eslintConfig": {
        "extends": [
            "../../.eslintrc.js"
